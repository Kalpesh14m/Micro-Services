package com.kalpesh.microservice.movieinfoservice.models;

public class Movie {
	private String name;
	private int movieId;

	public Movie(String name, int movieId) {
		this.name = name;
		this.movieId = movieId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getMovieId() {
		return movieId;
	}

	public void setMovieId(int movieId) {
		this.movieId = movieId;
	}

}
